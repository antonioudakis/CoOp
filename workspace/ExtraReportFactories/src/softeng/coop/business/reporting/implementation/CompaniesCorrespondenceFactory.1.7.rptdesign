            <xml-property name="queryText"><![CDATA[SELECT
company."id" AS companyId, company."email" AS "companyEmail", company."taxCode", company."taxDivision", company."webSite", 
(SELECT "text" FROM "Literal" WHERE multilingual_id = company.name_id ORDER BY "isDefault" DESC LIMIT 1) AS "companyName",
(SELECT "text" FROM "Literal" WHERE multilingual_id = centralBranch.name_id ORDER BY "isDefault" DESC LIMIT 1) AS "centralBranchName",
centralBranch."city", centralBranch."country", centralBranch."number", centralBranch."poBox" AS "postalCode", centralBranch."street",
centralBranch."fax", centralBranch."telephone",
contactPerson."name" AS "contactPersonName", contactPerson."surname" AS "contactPersonSurname", contactPerson."email" AS "contactPersonEmail",
contactCompanyPerson."position" AS "contactPersonPosition", contactCompanyPerson."salutation" AS "contactPersonSalutation",
contactPerson."gender" AS "contactPersonGender", contactPersonLanguage.name AS "contactPersonPerferredLanguage",
coop.id AS "coopId", coop."startDate",
(SELECT "text" FROM "Literal" WHERE multilingual_id = category.name_id ORDER BY "isDefault" DESC LIMIT 1) AS "categoryName"
FROM "Company" company
LEFT OUTER JOIN "Branch" centralBranch ON centralBranch.id = company.centralbranch_id
LEFT OUTER JOIN "Person" contactPerson ON contactPerson.id = company.contactperson_id
LEFT OUTER JOIN "CompanyPerson" contactCompanyPerson ON contactCompanyPerson.id = contactPerson.id
LEFT OUTER JOIN "Language" contactPersonLanguage ON contactPersonLanguage.id = contactPerson.preferredlanguage_id
LEFT OUTER JOIN "CoOp_Company" coopsToCompanies ON coopsToCompanies.companies_id = company.id AND coopsToCompanies.coops_id = ?
LEFT OUTER JOIN "CoOp" coop ON coop.ID = coopsToCompanies.coops_id
LEFT OUTER JOIN "Category" category ON category.id = company.category_id
LEFT OUTER JOIN "Category" rootCategory ON rootCategory.id = ?
WHERE 
(CASE WHEN ? THEN true ELSE coopsToCompanies.coops_id IS NOT NULL END)
AND (CASE WHEN rootCategory.id IS NOT NULL THEN (category."path" LIKE rootCategory."path" || '%') ELSE true END)]]></xml-property>
