            <xml-property name="queryText"><![CDATA[SELECT 
pers."name", pers."surname", 
--department_lit."text" as department_name,
(SELECT "text" FROM "Literal" WHERE multilingual_id = dep.name_id ORDER BY language_id ASC LIMIT 1) AS department_name,
st."taxId" as tax_id, pers."fatherName" as father_name, 
fs."name" as financialsourcename, fs."code" AS financialsourcecode,
scient_dir."surname" || ' ' || scient_dir."name" AS scientific_director,
scient_dir."gender" AS scientific_director_gender,
jo."startDate" as start_date, jo."endDate" as end_date,
(
	SELECT SUM("amount") 
	FROM "Payment" 
	WHERE 
	registration_id = reg.id AND
	source_id = fs.id AND jobpart_id IN
	(SELECT id FROM "JobPart" WHERE job_id = jo.id)
	AND "state" IN (0, 1)
	AND ((NOT ? AND "type" IN (1, 2)) OR (? AND "type" IN (3)))
) AS amount
FROM "FinancialSource" fs, "Person" pers
INNER JOIN "AuthenticatedUser" au ON au.id = pers.id
INNER JOIN "Student" st ON st.id = au.id
INNER JOIN "Registration" reg ON reg.student_id = st.id
INNER JOIN "Group" gr ON gr.id = reg.group_id
INNER JOIN "Job" jo ON jo.group_id = gr.id
INNER JOIN "CoOp" co ON co.id = reg.coop_id
LEFT OUTER JOIN "Person" scient_dir ON scient_dir.id = co.scientificdirector_id
INNER JOIN "Department" dep ON dep.id = au.department_id
--INNER JOIN "Literal" department_lit ON department_lit.multilingual_id = dep.name_id
WHERE --department_lit.language_id = 1 AND
co.id = ?
AND fs.id = ?
ORDER BY pers."surname";]]></xml-property>
